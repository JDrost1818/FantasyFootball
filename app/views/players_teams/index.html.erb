<h1>Listing players_teams</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @players_teams.each do |players_team| %>
      <tr>
        <td><%= link_to 'Show', players_team %></td>
        <td><%= link_to 'Edit', edit_players_team_path(players_team) %></td>
        <td><%= link_to 'Destroy', players_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Players team', new_players_team_path %>
